[user]
	name = 
	email = 
[alias]
	tree = log --graph --decorate --pretty=oneline --abbrev-commit
	cm = commit -m
	st = status
	br = branch
	ck = checkout
	
	ck- = checkout -
	ckm = checkout master
	cma = !git add -A && git commit -m
	cmd = commit --amend -m
	dfl = diff --cached HEAD^
	gcu = config --global user.name
	gce = config --global user.email
	pop = push -u origin
	rao = remote add origin

	fett = clone --bare
[merge]
	tool = vimdiff
[push]
	default = simple
[core]
	editor = vim
